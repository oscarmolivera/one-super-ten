<%= turbo_stream.replace "call_up_frame_#{@call_up.match.id}", "" %>

<%= turbo_stream.replace "call_up_button_#{@call_up.match.id}" do %>
  <button type="button"
          id="call_up_button_<%= @call_up.match.id %>"
          class="btn btn-outline-primary btn-sm"
          data-controller="turbo-loader"
          data-action="click->turbo-loader#load"
          data-turbo-loader-url-value="<%= edit_or_new_call_up_url(@call_up.match) %>"
          data-turbo-loader-frame-value="call_up_frame_<%= @call_up.match.id %>">
    <i class="bi bi-people-fill me-1"></i>
    Editar Convocatoria
  </button>
<% end %>