<h1>Edit User for <%= current_tenant.name %></h1>
<%= form_for [:admin, @user] do |f| %>
  <p>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </p>
  <p>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </p>
  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :role %>
    <%= f.select :role, User.roles.keys.map { |role| [role.humanize, role] } %>
  </p>
  <p>
    <%= f.submit "Update User" %>
  </p>
<% end %>